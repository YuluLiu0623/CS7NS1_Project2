tmux new-session -d -s EXAMPLE
tmux new-window -d -n server
tmux new-window -d -n ballast
tmux new-window -d -n barometer
tmux new-window -d -n camera
tmux new-window -d -n compass
tmux new-window -d -n motor
tmux new-window -d -n positioning
tmux new-window -d -n power
tmux new-window -d -n sampler
tmux send-keys -t Drone:server "PYTHONPATH=. python3 ./examples/noed.py" Enter
tmux send-keys -t Drone:ballast "PYTHONPATH=. python3 ./examples/water_quality_sensor.py" Enter
tmux send-keys -t Drone:barometer "PYTHONPATH=. python3 ./examples/water_flow_sensor.py" Enter
tmux send-keys -t Drone:camera "PYTHONPATH=. python3 ./examples/temperature_sensor.py" Enter
tmux send-keys -t Drone:compass "PYTHONPATH=. python3 ./examples/sonar_sensor.py" Enter
tmux send-keys -t Drone:motor "PYTHONPATH=. python3 ./examples/emf_sensor.py" Enter
tmux send-keys -t Drone:positioning "PYTHONPATH=. python3 ./examples/communication_modulator.py" Enter
tmux send-keys -t Drone:power "PYTHONPATH=. python3 ./examples/lighting_controller.py" Enter
tmux send-keys -t Drone:sampler "PYTHONPATH=. python3 ./examples/submersible_pump_controller.py" Enter